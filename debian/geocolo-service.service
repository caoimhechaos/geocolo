[Unit]
Description=geocolo-service geolocation services

[Service]
Type=simple
User=_geocolo
Group=_geocolo
EnvironmentFile=-/etc/default/geocolo-service
ExecStart=/usr/bin/geocolo-service --listen-addr=$GEOCOLO_LISTEN --config=$GEOCOLO_CONFIGPATH $GEOCOLO_SERVICE_FLAGS

[Install]
WantedBy=multi-user.target
